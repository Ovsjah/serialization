<html>
<head>
  <meta charset='utf-8'>
  <title>Scratch</title>
</head>
<body>
  <h1>Users Info</h1>
  <table>
    <tr><th>Name</th><th>Phone Number</th></tr>
      <% name_phone.each do |name, phone| %>
        <tr>
          <td><%= name %></td>
          <td><%= phone %></td>
        </tr>
      <% end %>
  </table>
  <h2>Registration statistic</h2>
  <p><%= find_max_reg_rate(reg_rate_hour) %></p>
  <p><%= find_max_reg_rate(reg_rate_day) %></p>
</body>
</html>
